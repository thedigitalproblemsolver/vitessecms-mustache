{{#block.hasItems}}
    <ol class="breadcrumb">
        <li class="{{_id}} breadcrumb-item">
            <a href="{{BASE_URI}}" class="fa fa-home"></a>
        </li>
        {{#block.items}}
        <li class="{{_id}} breadcrumb-item" >
            <a href="{{slug[]}}" >
                {{name[]}}
            </a>
        </li>
        {{/block.items}}
    </ol>
{{/block.hasItems}}
